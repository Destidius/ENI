package fr.eni.demonstration;

public class Demonstration {

	public static void main(String[] args) {
		
		int uneValeur = 777;
		
		if(uneValeur == 444)
		{
			System.out.println("La variable est égal à 444");
		}

		if(uneValeur == 444)
		{
			System.out.println("La variable est égal à 444");
		}else
		
		
		if(uneValeur == 222)
		{
			System.out.println("La variable est égal à 444");
		}else if(uneValeur == 333)
		{
			System.out.println("La variabl est égale à 333");
		}else {
			System.out.println("Pas 222, 333 et 444");
		}

	}

}
