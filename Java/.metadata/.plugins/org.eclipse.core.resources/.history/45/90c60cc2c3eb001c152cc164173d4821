package fr.eni.demonstration;

public class Demonstration {

	public static void main(String[] args) {
		
		int uneValeur = 555;
		
		if(uneValeur == 444)
		{
			System.out.println("La variable est égal à 444");
		}

		if(uneValeur == 444)
		{
			System.out.println("La variable est égal à 444");
		}else
		{
			System.out.println("La variable n'est pas égal à 444");
		}

	}

}
