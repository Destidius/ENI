package fr.eni.tpmax;

public class TpMax {

	public static void main(String[] args) {
		float z = compare(5.0F,6.0F);
		System.out.println(z);
	}
	
	public static float compare(float a, float b)
	{
		if (a > b) {
			return a;
		}
		else {
			return b;
		}
		
	public static float compare2(float a, float b)
	{
		if (a == b) {
			return 0;
		}
		else if(a > b){
			return 1;
		}
		else {
			return -1;
		}
			
			
	}

}
